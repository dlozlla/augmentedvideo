<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel='stylesheet' href='<%= context %>/commons/bower_components/bootstrap/dist/css/bootstrap.min.css'>
    <link rel="stylesheet" href="<%= context %>/commons/bower_components/bootstrap-select/dist/css/bootstrap-select.min.css" type="text/css" media="screen" />

    <link rel="stylesheet" type="text/css" href="<%= context %>/themes/stylesheets/stylesheet.css" media="screen">
    <link rel="stylesheet" type="text/css" href="<%= context %>/themes/stylesheets/github-light.css" media="screen">
    <link rel="stylesheet" type="text/css" href="<%= context %>/themes/stylesheets/print.css" media="print">

    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <script type="text/javascript" src='<%= context %>/commons/bower_components/jquery/dist/jquery.min.js'></script>
    <script type="text/javascript" src='<%= context %>/commons/bower_components/bootstrap/dist/js/bootstrap.min.js'></script>
    <script type="text/javascript" src='<%= context %>/commons/bower_components/bootstrap-select/dist/js/bootstrap-select.min.js'></script>
    <script type="text/javascript" src="<%= context %>/themes/javascripts/main.js"></script>

    <title>Telef√≥nica Agora</title>
  </head>

  <body>
    <header>
      <div class="inner-container">
        <h1>Telefonica</h1>
        <h2>All your code, all in one place.</h2>
        <%-blocks.header%>
      </div>
    </header>

    <div id="content-wrapper">
      <div class="inner-container clearfix">
        <section id="main-content">
          <%- body %>
        </section>
      </div>
    </div>

    <script type="text/javascript">
      var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
      document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
    <script type="text/javascript">
      try {
        var pageTracker = _gat._getTracker("UA-65860146-1");
      pageTracker._trackPageview();
      } catch(err) {}
    </script>

  </body>
</html>
